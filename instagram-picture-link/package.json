{
  "name": "duplicate-tab-skipper",
  "version": "1.0.0",
  "description": "Add link to maxres instagram pictures",
  "author": "Sebastian Gr√ºb",
  "license": "MIT",
  "scripts": {
    "tsc": "tsc",
    "build": "tsc && cp ./src/resources/* ./build",
    "watch": "npm-watch"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.127",
    "npm-watch": "^0.7.0",
    "typescript": "^4.1.3"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,json,html,png",
      "quiet": true,
      "delay": 1000
    }
  }
}
